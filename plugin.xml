<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            class="com.archiplugin.projectcreator.extension.ProjectCreationMenuExtensionContributionFactory"
            locationURI="popup:com.archimatetool.editor.treeModelView.new_menu?after=new_additions">
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:com.archimatetool.editor.treeModelView?after=com.archimatetool.editor.treeModelView.new_menu">
         <menu
               id="com.archiplugin.projectcreator.lifecycle_menu"
               label="%menu.label.move_to_next_lifecycle"
               tooltip="Configure lifecycles in the preferences">
            <dynamic
                  class="com.archiplugin.projectcreator.extension.ProjectLifecycleMenuContributionItem"
                  id="com.archiplugin.projectcreator.lifecycle_items">
            </dynamic>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.archiplugin.projectcreator.preferences.ProjectCreationPreferencesPage"
            id="com.archiplugin.projectcreator.ProjectCreatorPreferencePage"
            name="%view.name">
      </page>
   </extension>

</plugin>
